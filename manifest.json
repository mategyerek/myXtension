{
    "manifest_version": 3,
   
    "name": "myXtension",
    "version": "0.2.0",
    "description": "My extension",
    "content_scripts": [
        {
          "js": ["messenger.js", "ly_eliminator.js"],
          "matches": ["https://www.messenger.com/*"]
        },
        {
          "js": ["nytimes.js"],
          "matches": ["https://www.nytimes.com/*"]
        },
        {
          "js": ["washingtonpost.js"],
          "matches": ["https://www.washingtonpost.com/*"]
        }
      ]
}