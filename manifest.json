{
    "manifest_version": 3,
   
    "name": "myXtension",
    "version": "0.2.0",
    "description": "My extension",
    "content_scripts": [
        
        {
          "js": ["ly_eliminator.js"],
          "matches": ["https://www.messenger.com/*", "https://telex.hu/*", "https://index.hu/*", "https://www.origo.hu/*", "https://444.hu/*", "https://888.hu/*", "https://www.facebook.com/*"]
        },
        {
          "js": ["messenger.js"],
          "matches": ["https://www.messenger.com/*"]
        },
        {
          "js": ["nytimes.js"],
          "matches": ["https://www.nytimes.com/*"]
        },
        {
          "js": ["washingtonpost.js"],
          "matches": ["https://www.washingtonpost.com/*"]
        }
      ]
}